{
  "name": "minimize-js",
  "description": "Minimize your files without making a bundle",
  "version": "1.3.0",
  "main": "lib/index.js",
  "bin": {
    "minimize-js": "lib/index.js"
  },
  "files": [
    "lib",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "start": "tsc --watch",
    "test": "jest",
    "debug": "node --inspect ./lib/index.js lib -w -s -d",
    "build": "tsc -b -v -f",
    "postbuild": "node ./lib/index.js lib -w -s -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Randagio13/minimize-js.git"
  },
  "keywords": [
    "minimize-js",
    "minimize-json",
    "minimize",
    "minify",
    "minify-js",
    "minify-json"
  ],
  "author": "Alessandro Casazza",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Randagio13/minimize-js/issues"
  },
  "homepage": "https://github.com/Randagio13/minimize-js#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.7",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/glob": "^8.0.0",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "jest": "^29.3.1",
    "semantic-release": "^19.0.5",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "commander": "^9.4.1",
    "dts-minify": "^0.3.1",
    "esbuild": "^0.16.13",
    "glob": "^8.0.3",
    "pretty-bytes": "^6.0.0",
    "progress-barjs": "^2.2.1",
    "tslib": "^2.4.1"
  },
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "main",
      "next",
      "next-major",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      [
        "@semantic-release/github",
        {
          "addReleases": "top"
        }
      ]
    ]
  }
}
